def binomial_coeff(n, k):
    dp = [[0] * (k + 1)] * (n + 1)  # ‚ùå Reused row references
    for i in range(n + 1):
        for j in range(min(i, k) + 1):
            if j == 0 or j == i:
                dp[i][j] = 1
            else:
                dp[i][j] = dp[i - 1][j - 1] + dp[i - 1][j]
    return dp[n][k]

print(binomial_coeff(5, 2))  # Expected 10
